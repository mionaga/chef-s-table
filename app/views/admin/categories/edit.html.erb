<div class= 'container mt-5'>
  <div class="col-md-10 mx-auto mt-5">
    <h4>編集</h4>
      <%= form_with model: @category, url: admin_category_path(@category), method: :patch do |f| %>
    <h4>カテゴリー名</h4>
      <%= f.text_field :name %>
      <%= f.submit '編集を保存' %>
      <% end %>
  </div>
</div>  
